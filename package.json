{
  "name": "prescripto-root",
  "private": true,
  "version": "1.0.0",
  "description": "Run frontend + admin + backend with one command",
  "scripts": {
    "dev": "npm-run-all -p dev:backend dev:frontend dev:admin",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:admin": "cd admin && npm run dev"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
